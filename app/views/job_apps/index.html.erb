<p id="notice"><%= notice %></p>

<h1>Job Apps</h1>

<table>
  <thead>
    <tr>
      <th>First</th>
      <th>Name</th>
      <th>Last</th>
      <th>Name</th>
      <th>Phone</th>
      <th>Job</th>
      <th>Role</th>
      <th>Place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_apps.each do |job_app| %>
      <tr>
        <td><%= job_app.First %></td>
        <td><%= job_app.Name %></td>
        <td><%= job_app.Last %></td>
        <td><%= job_app.Name %></td>
        <td><%= job_app.Phone %></td>
        <td><%= job_app.Job %></td>
        <td><%= job_app.Role %></td>
        <td><%= job_app.Place %></td>
        <td><%= link_to 'Show', job_app %></td>
        <td><%= link_to 'Edit', edit_job_app_path(job_app) %></td>
        <td><%= link_to 'Destroy', job_app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job App', new_job_app_path %>
